# Editor configuration, see https://editorconfig.org
root = true

# General (unknown files)
[*]
charset = utf-8
indent_style = tab
indent_size = tab
tab_width = 4
insert_final_newline = true
trim_trailing_whitespace = true
end_of_line = lf

# HTML
indent_size = unset
insert_final_newline = unset
tab_width = unset
indent_style = unset
insert_final_newline = unset

# XML
[*.targets;*.xml]
charset = utf-8
max_line_length = off
quote_type = double
trim_trailing_whitespace = true
insert_final_newline = false

# YAML, JSON, Markdown
[*.{yml,yaml,md,json,js}]
charset = utf-8
indent_style = space
tab_width = 2
max_line_length = off
quote_type = single
trim_trailing_whitespace = true
insert_final_newline = false

# Properties
[.gitconfig]
charset = utf-8
max_line_length = off
quote_type = double
trim_trailing_whitespace = true
insert_final_newline = false

[*.resjson]
insert_final_newline = false

# C#
[*.cs]

charset = utf-8
indent_style = tab
indent_size = tab
tab_width = 4
insert_final_newline = true
trim_trailing_whitespace = true
end_of_line = crlf

